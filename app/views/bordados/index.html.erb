<h1>Lista de Fechas de Bordados</h1>

<table>
  <tr>
    <th>Fecha de envio</th>
    <th>Fecha de llegada</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @bordados.each do |bordado| %>
  <tr>
    <td><%= bordado.fecha_envio %></td>
    <td><%= bordado.fecha_llegada %></td>
    <td><%= link_to 'Mostrar', bordado %></td>
    <td><%= link_to 'Editar', edit_bordado_path(bordado) %></td>
    <td><%= link_to 'Eliminar', bordado, method: :delete, data: { confirm: 'Estas Seguro?' } %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'Nuevo', new_bordado_path %>
